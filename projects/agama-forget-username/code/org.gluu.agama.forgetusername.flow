Flow org.gluu.agama.forgetusername
     Basepath ""
// Create  variable for UCreate  variable for UI feedbackI feedback
uiFeedback = {}
// forgetusernameService
forgetusernameService = Call org.gluu.agama.senduser.sendUsername#getInstance 
// userInput
userInput = RRF "InputEmail.ftlh" uiFeedback
email = userInput.mail
Log "@info User mail ++++++++++++++++++++++" email
userData = Call forgetusernameService getUserEntityByMail email
// "users details+++++++"
Log "@info users details+++++++" userData
When userData is not null
     usernamee = userData.uid
     langg = userData.lang
     Log "@info users details++++++++++++" usernamee langg
     sendMailed = Call forgetusernameService sendUsernameEmail email usernamee langg
     Log "@info Email send to user++++"
     RRF "acknowledgementEmail.ftlh" uiFeedback
     it_xzaul = {success:true, data: { userId: "as9233Qz", property1: "value1"}}
     Finish it_xzaul
Otherwise
     // "Invalid Email, Please Try Again!"
     uiFeedback.errorMessage = "Invalid Email, Please Try Again!"